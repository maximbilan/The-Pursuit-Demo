// GUITemplate definitions to make it easier to specify common pieces of UI

//-----------------------------------------------------------------------------
// text_label
// This is a GUILabel aligned top left of bounds, white in small text font
//-----------------------------------------------------------------------------
GUITemplate
{
	name		text_label

	GUILabel
	{
		name		text_label

		align		{ left top }
		colour		white
		font		tahoma_medium
	}
}

