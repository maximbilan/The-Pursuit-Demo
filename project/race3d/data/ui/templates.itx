//-----------------------------------------------------------------------------
// button_label
// This is a GUILabel used for adding labels to GUIButton instances
//-----------------------------------------------------------------------------
GUITemplate
{
	name		button_label

	GUILabel
	{
		name		button_label

		bounds		{ centre centre 50% 50% 100% 100% }
		align		{ centre centre }
		colour		white
		font		tahoma_medium
	}
}

