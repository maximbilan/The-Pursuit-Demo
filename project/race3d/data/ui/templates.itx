//-----------------------------------------------------------------------------
// button_backing
// The GUIFrames required for a standard buttons up and down images
//-----------------------------------------------------------------------------
GUITemplate
{
	name	button_backing

	GUIFrame
	{
		name	button_up
		bounds	{ left top 0 0 100% 100% }

		GUIBorder
		{
			name		button_up_border
			bounds_tl	{ left top 0 0 8 8 }
			bounds_br	{ left top -8 -8 8 8 }
			material	popup
			uv			{ 0 0 8 8 56 56 64 64 }
			colour		button_up
		}
	}

	GUIFrame
	{
		name	button_down
		bounds	{ left top 0 0 100% 100% }

		GUIBorder
		{
			name		button_down_border
			bounds_tl	{ left top 0 0 8 8 }
			bounds_br	{ left top -8 -8 8 8 }
			material	popup
			uv			{ 0 0 8 8 56 56 64 64 }
			colour		button_down
		}
	}

	GUIFrame
	{
		name	button_disabled
		bounds	{ left top 0 0 100% 100% }

		GUIBorder
		{
			name		button_disabled_border
			bounds_tl	{ left top 0 0 8 8 }
			bounds_br	{ left top -8 -8 8 8 }
			material	popup
			uv			{ 0 0 8 8 56 56 64 64 }
			colour		button_disabled
		}
	}
}

